<?xml version="1.0" encoding="utf-8"?>

<pyblio-db>
<pyblio-schema>
 <attribute id="author" type="person">
  <name>Author</name>
 </attribute>

 <attribute id="doctype" type="txo" group="doctype">
  <name>Document Type</name>
 </attribute>

 <attribute id="id" type="id">
  <name>BibTeX Key</name>
 </attribute>

 <attribute id="journal" type="text">
  <name>Journal</name>
 </attribute>

 <attribute id="note" type="text">
  <name>Note</name>
 </attribute>

 <attribute id="number" type="text">
  <name>Number</name>
 </attribute>

 <attribute id="title" type="text">
  <name>Title</name>
 </attribute>

</pyblio-schema>
 <txo-group id="doctype">
  <txo-item id="1">
   <name>Article</name>
  </txo-item>
 </txo-group>

<header>This is a comment
This is a { comment too
{mee too}</header>
 <entry id="1">

 <native type="bibtex">@article{toto,
   author = {Gobry, Frederic and First Last},
   journal = "My journal",
   note = {a \{ and a \}},
   number = 123,
   title = {This is a title}
}</native>

  <attribute id="author">
   <person first="Frederic" last="Gobry"/>
   <person first="First" last="Last"/>
  </attribute>
  <attribute id="doctype">
   <txo group="doctype" id="1"/>
  </attribute>
  <attribute id="id">
   <id value="toto"/>
  </attribute>
  <attribute id="journal">
   <text>My journal</text>
  </attribute>
  <attribute id="note">
   <text>a { and a }</text>
  </attribute>
  <attribute id="number">
   <text>123</text>
  </attribute>
  <attribute id="title">
   <text>This is a title</text>
  </attribute>
 </entry>
</pyblio-db>
